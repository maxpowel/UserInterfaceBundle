        <div id="toolbar" class="ui-widget-header ui-corner-all ui-helper-clearfix">

<span id="buttons">
		<input type="radio" id="but-start" name="buttons" checked="checked" /><label style="top: 3px; left:5px" for="but-start">{{ 'header.start'|trans({}, 'UserInterfaceBundle') }}</label>

		<input type="radio" id="but-profile" name="buttons" /><label style="top: 3px;" for="but-profile">{{ 'header.profile'|trans({}, 'UserInterfaceBundle') }}</label>
		<input type="radio" id="but-search" name="buttons" /><label style="top: 3px; left:-5px" for="but-search">{{ 'header.search'|trans({}, 'UserInterfaceBundle') }}</label>
	</span>
		
	<input class="text ui-widget-content ui-corner-all" type="text" />
	<div class="chat" id="chat"></div>

			
	<div class="dock" id="dock">
		<div class="dock-container">

		  <a class="dock-item" rel="history" href="#album">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/photo.png'
				output='images/photo.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.photos'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.photos'|trans({}, 'UserInterfaceBundle') }}</span></a>
		  
		  <a class="dock-item" rel="history" href="#calendar">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/calendar.png'
				output='images/calendar.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.agenda'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.agenda'|trans({}, 'UserInterfaceBundle') }}</span></a>
 
		  <a class="dock-item" rel="history" href="#privateMessage">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/message.png'
				output='images/message.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.messages'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.messages'|trans({}, 'UserInterfaceBundle') }}</span></a>
		  
		  <a class="dock-item" href="javascript:void(0)" onclick="$.wixet.uploadPhotoDialog()">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/photo-upload.png'
				output='images/photo-upload.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.photoUpload'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.photoUpload'|trans({}, 'UserInterfaceBundle') }}</span></a>
		  
		  <a class="dock-item" rel="history" href="#gadgets">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/opensocial.png'
				output='images/opensocial.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.gadgets'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.gadgets'|trans({}, 'UserInterfaceBundle') }}</span></a>
		  
		  <a class="dock-item" rel="history" href="#settings">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/profile.png'
				output='images/profile.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.account'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.account'|trans({}, 'UserInterfaceBundle') }}</span></a>
		  
		  <a class="dock-item" id="dock-exit" href="/logout">
		  {% image '@UserInterfaceBundle/Resources/public/images/dock/exit.png'
				output='images/exit.png'
				%}
				<img src="{{ asset_url }}" alt="{{ 'header.exit'|trans({}, 'UserInterfaceBundle') }}" />
		  {% endimage %}
		  <span>{{ 'header.exit'|trans({}, 'UserInterfaceBundle') }}</span></a>
		</div>

	</div>

<div style="float:right">
</div>


</div>

